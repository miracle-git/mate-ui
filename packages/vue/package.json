{
  "name": "@mate-ui/vue",
  "version": "1.0.0",
  "title": "Mate UI Vue",
  "description": "An enterprise-level Mate design language and Vue-based implementation",
  "keywords": [
    "vue",
    "vue2",
    "vue3",
    "mate",
    "mate-ui",
    "design",
    "vueComponent",
    "component",
    "components",
    "ui",
    "framework",
    "frontend"
  ],
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/mate.min.js",
  "typings": "es/index.d.ts",
  "files": [
    "dist",
    "lib",
    "es",
    "scripts",
    "vetur",
    "typings/global.d.ts",
    "locale"
  ],
  "scripts": {
    "predev": "pnpm run version && pnpm run token",
    "dev": "pnpm run routes && vite serve docs",
    "routes": "node docs/scripts/generate-routes.js",
    "version": "node scripts/version.js",
    "tsc": "tsc --noEmit",
    "vue-tsc": "vue-tsc --noEmit",
    "lint": "pnpm run tsc && pnpm run lint:demo && pnpm run lint:md && pnpm run lint:script && pnpm run lint:docs",
    "lint:src": "eslint --fix --ext .jsx,.js,.ts,.tsx ./src",
    "lint:demo": "eslint --fix src/components/*/demo/*.vue",
    "lint:md": "eslint --fix *.md",
    "lint:script": "eslint . --ext '.js,.jsx,.ts,.tsx'",
    "lint:docs": "eslint --fix -c ./.eslintrc.js --ext .jsx,.js,.ts,.tsx,vue ./docs",
    "lint:style": "stylelint \"{docs,src}/**/*.less\" --syntax less"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:miracle-git/mate-ui.git"
  },
  "license": "MIT",
  "homepage": "https://www.mate-ui.com/",
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "engines": {
    "node": ">=12.22.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.10.5",
    "@mate-ui/core": "workspace:^",
    "@mate-ui/icon": "workspace:^",
    "@mate-ui/locale": "workspace:^"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-member-expression-literals": "^7.8.3",
    "@babel/plugin-transform-object-assign": "^7.8.3",
    "@babel/plugin-transform-property-literals": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.10.5",
    "@babel/plugin-transform-template-literals": "^7.8.3",
    "@babel/plugin-transform-typescript": "^7.12.1",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.10.4",
    "@types/fs-extra": "^9.0.8",
    "@types/lru-cache": "^5.1.0",
    "@types/markdown-it": "^10.0.2",
    "@types/node": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@vitejs/plugin-legacy": "^2.2.0",
    "@vitejs/plugin-vue": "^3.0.0",
    "@vitejs/plugin-vue-jsx": "^2.0.0",
    "@vue/cli-plugin-eslint": "^5.0.0",
    "@vue/eslint-config-typescript": "^11.0.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^28.1.2",
    "babel-loader": "^8.0.0",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-inline-import-data-uri": "^1.0.1",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-require-context": "^0.1.1",
    "chalk": "^4.1.1",
    "cz-git": "^1.3.8",
    "diacritics": "^1.3.0",
    "escape-html": "^1.0.3",
    "eslint": "^8.3.0",
    "eslint-plugin-html": "^6.0.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^26.0.0",
    "eslint-plugin-markdown": "^2.0.0",
    "eslint-plugin-no-explicit-type-exports": "^0.12.0",
    "eslint-plugin-vue": "^9.17.0",
    "fast-glob": "^3.2.7",
    "fs-extra": "^10.0.0",
    "glob": "^7.1.2",
    "globby": "^11.1.0",
    "gray-matter": "^4.0.3",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^8.0.4",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^2.0.0",
    "markdown-it-table-of-contents": "^0.5.2",
    "prismjs": "^1.23.0",
    "progress": "^2.0.3",
    "slash": "^3.0.0",
    "ts-jest": "^28.0.5",
    "ts-loader": "^9.1.0",
    "tsx": "^3.12.10",
    "typedoc": "^0.23.25",
    "typescript": "~4.9.3",
    "vite": "^3.0.0",
    "vue": "^3.2.0",
    "vue-eslint-parser": "^9.3.1"
  },
  "sideEffects": [
    "docs/*",
    "dist/*",
    "*.vue",
    "*.md",
    "*.css"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./scripts/commitizen.js"
    }
  },
  "web-types": "vetur/web-types.json"
}
