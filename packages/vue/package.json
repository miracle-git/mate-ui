{
  "name": "@mate-ui/vue",
  "version": "1.0.0",
  "description": "An enterprise-level Mate-UI design language and Vue-based implementation",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "dist/mate.min.js",
  "typings": "es/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:miracle-git/mate-ui.git"
  },
  "license": "MIT",
  "homepage": "https://www.mate-ui.com/",
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "files": [
    "dist",
    "lib",
    "es",
    "scripts",
    "vetur",
    "typings/global.d.ts",
    "locale"
  ],
  "keywords": [
    "mate-ui",
    "mate",
    "vue",
    "vue3",
    "design",
    "vueComponent",
    "component",
    "components",
    "ui",
    "framework",
    "frontend"
  ],
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "gen": "node script/transfer.mjs"
  },
  "engines": {
    "node": ">=12.22.0"
  },
  "sideEffects": [
    "docs/*",
    "*.vue",
    "*.md",
    "dist/*",
    "*.css"
  ],
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./scripts/commitizen.js"
    }
  },
  "dependencies": {
    "@mate-ui/core": "workspace:^",
    "@mate-ui/icon": "workspace:^",
    "@mate-ui/locale": "workspace:^"
  }
}
