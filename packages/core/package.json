{
  "name": "@mate-ui/core",
  "version": "0.0.0",
  "description": "核心基础库",
  "author": "Miracle He <hmiinyu@sina.com>",
  "homepage": "https://github.com/miracle-git/mate-ui#readme",
  "license": "MIT",
  "main": "lib/mat.esm.js",
  "scripts": {
    "clean": "rimraf ./lib",
    "dev": "NODE_ENV=development rollup -c -w",
    "build": "npm run clean && npm run test && NODE_ENV=production rollup -c",
    "generator": "node scripts/doc2test.js",
    "test": "jest --verbose --runInBand --coverage",
    "release": "echo $npm_package_version"
  },
  "jest": {
    "roots": [
      "<rootDir>/src"
    ],
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.js?$",
    "moduleFileExtensions": [
      "js",
      "json",
      "node"
    ]
  },
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/miracle-git/mate-ui.git"
  },
  "bugs": {
    "url": "https://github.com/miracle-git/mate-ui/issues"
  },
  "dependencies": {
    "crypto-js": "^4.1.1"
  },
  "devDependencies": {
    "babel-jest": "^23.6.0",
    "babel-polyfill": "^6.26.0",
    "cross-env": "^7.0.2",
    "jest": "^26.4.2",
    "jest-html-reporter": "^3.2.0",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^2.6.2",
    "rollup": "^1.27.5",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-serve": "^0.6.0",
    "rollup-plugin-uglify": "^6.0.0",
    "uglify-es": "^3.3.9"
  }
}
